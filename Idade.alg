algoritmo "semnome"
// Função :
// Autor :
// Data : 3/25/2025
// Seção de Declarações 
var
   ano_atual, ano_nasc, idade: Inteiro
inicio
      escreval("Em que ano estamos?")
      Leia(ano_atual)
      escreval("Qual o seu ano de nascimento?")
      Leia(ano_nasc)
      idade <- ano_atual - ano_nasc
      escreval("Minha idade eh de: ", idade)
fimalgoritmo
